{
    "name": "local-npm-config",
    "version": "3.0.0-0",
    "description": "Manage your local npm configuration across multiple machines easily!",
    "bin": {
        "local-npm-config": "src/cli.js",
        "localnpm": "src/cli.js"
    },
    "scripts": {
        "lint": "standard --fix",
        "localLaunch": "node src/cli.js",
        "release": "np",
        "prepublishOnly": "npm test",
        "test": "standard && tap"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18",
        "@types/yargs": "^17.0.32",
        "get-port-please": "^3.1.1",
        "np": "^7.7.0",
        "nyc": "^15.1.0",
        "proxyquire": "^2.1.3",
        "standard": "^17.1.0",
        "tap": "^18.6.1"
    },
    "dependencies": {
        "@npmcli/config": "^8.0.2",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "flat": "^5.0.2",
        "fuse.js": "^6.6.2",
        "inquirer": "^8.2.6",
        "inquirer-autocomplete-prompt": "^2.0.1",
        "prepend-cwd": "^2.0.0",
        "signale": "^1.4.0",
        "spdx-license-list": "^6.8.0",
        "undici": "^5.28.1",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">=18"
    },
    "files": [
        "src/**/*.js",
        "src/io/config.schema.json"
    ],
    "keywords": [
        "npm",
        "npmjs",
        "local",
        "configuration",
        "config",
        "management",
        "local configuration",
        "configuration files",
        "config files"
    ],
    "author": {
        "name": "Nicola Dal Maso",
        "url": "https://github.com/niktekusho/"
    },
    "bugs": {
        "url": "https://github.com/niktekusho/local-npm-config/issues"
    },
    "contributors": [
        {
            "name": "Nicola Dal Maso",
            "url": "https://github.com/niktekusho/"
        }
    ],
    "homepage": "https://github.com/niktekusho/local-npm-config",
    "repository": {
        "type": "git",
        "url": "https://github.com/niktekusho/local-npm-config"
    }
}
