{
    "name": "local-npm-config",
    "version": "1.0.0-1",
    "description": "Manage your local npm configuration across multiple machines easily!",
    "bin": {
        "local-npm-config": "src/cli.js",
        "localnpm": "src/cli.js"
    },
    "scripts": {
        "coverage": "nyc --all --reporter lcov --reporter text ava",
        "localLaunch": "node src/cli.js",
        "release": "np",
        "prepublishOnly": "yarpm test",
        "test": "xo && ava"
    },
    "license": "MIT",
    "devDependencies": {
        "ava": "^5.1.1",
        "get-port": "^5.1.0",
        "mock-require": "^3.0.3",
        "np": "^7.6.3",
        "nyc": "^15.0.0",
        "tempy": "^0.3.0",
        "xo": "^0.53.1",
        "yarpm": "^0.2.1"
    },
    "dependencies": {
        "ajv": "^6.10.2",
        "download": "^7.1.0",
        "execa": "^4.0.0",
        "flat": "^5.0.0",
        "fuse.js": "^3.3.0",
        "inquirer": "^7.0.1",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "meow": "^6.0.0",
        "meow-shorts": "^1.0.0",
        "prepend-cwd": "^2.0.0",
        "signale": "^1.3.0",
        "spdx-license-list": "^6.1.0"
    },
    "engines": {
        "node": ">=8"
    },
    "files": [
        "src/**/*.js",
        "src/io/config.schema.json"
    ],
    "keywords": [
        "npm",
